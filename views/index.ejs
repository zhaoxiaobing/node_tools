<!doctype html>
<html lang="zh-CN">
<head>
    <meta charset="UTF-8">
    <meta name="renderer" content="webkit">
    <meta http-equiv="cache-control" content="no-cache">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <link rel="stylesheet" type="text/css" href="/public/css/bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="/public/css/style.css" />
    <title>ejs</title>
</head>
<body>


<header id="header">
    <button class="btn btn-success" type="button" data-new-topic>新建</button>
    <div class="progressBar">
        <span></span>
    </div>
    <strong class="progressBarNum"></strong>
    <form action="" method="post" id="fileForm" enctype="multipart/form-data">
        <input type="text" name="name" data-name />
        <input type="file" name="filePic" data-file-topic />
    </form>
</header>

<table cellspacing="0" cellpadding="0" class="table table-bordered">
    <tr>
        <th>标题</th>
        <th>时间</th>
        <th>操作</th>
    </tr>
    <% if(list){%>
    <% for(var i =0;i<list.length;i++){%>
    <tr>
        <td><%=list[i].name %></td>
        <td><%=list[i].time %></td>
        <td><a href="/main/<%=list[i].path %>" target="_blank">编辑</a></td>
    </tr>
    <%}%>
    <%}%>



</table>



</body>
<script type="text/javascript" src="/public/js/jquery.js"></script>
<script type="text/javascript" src="/public/js/index.js"></script>
</html>